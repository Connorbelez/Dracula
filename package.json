{
    "name": "dracula-enhanced",
    "version": "1.0.0",
    "displayName": "Dracula Enhanced",
    "homepage": "https://github.com/connor/dracula-enhanced",
    "description": "Enhanced Dracula theme with improved semantic highlighting for parameters and properties. ðŸ¦‡",
    "publisher": "connor",
    "license": "MIT",
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "mkdir -p ./bin && vsce package -o ./bin/dracula.vsix",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Connor"
    ],
    "contributors": [
        "Connor"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/connor/dracula-enhanced.git"
    },
    "bugs": {
        "url": "https://github.com/connor/dracula-enhanced/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "dracula",
        "dark",
        "theme",
        "color-theme",
        "enhanced",
        "semantic",
        "typescript",
        "javascript"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#3c4557",
        "theme": "dark"
    },
    "contributes": {
        "themes": [
            {
                "label": "Dracula Enhanced",
                "uiTheme": "vs-dark",
                "path": "./theme/dracula.json"
            },
            {
                "label": "Dracula Enhanced Soft",
                "uiTheme": "vs-dark",
                "path": "./theme/dracula-soft.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0",
        "prettier": "^3.0.0",
        "tinycolor2": "^1.6.0",
        "vsce": "^2.15.0"
    }
}
